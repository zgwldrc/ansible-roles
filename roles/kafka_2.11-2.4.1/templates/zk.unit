[Unit]
Description={{SYSTEMD_NAME}}
After=network.target

[Service]
Type=simple
User={{USER}}
Group={{GROUP}}
WorkingDirectory={{HOME}}
Environment=JAVA_HOME={{JAVA_HOME}}
Environment=CLASSPATH=.:{{JAVA_HOME}}/lib:{{JAVA_HOME}}/jre/lib
Environment=PATH={{JAVA_HOME}}/bin:/usr/bin:/bin:/sbin:/usr/sbin
ExecStart={{BINDIR}}/zookeeper-server-start.sh config/zookeeper.properties
[Install]
WantedBy=multi-user.target