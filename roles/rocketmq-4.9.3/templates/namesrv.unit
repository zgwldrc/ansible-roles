[Unit]
Description=rocketmq namesrv service
After=network.target

[Service]
Type=simple
EnvironmentFile={{home}}/conf/.env
PassEnvironment=JAVA_HOME PATH
ExecStart={{ home }}/bin/mqnamesrv
ExecStop={{ home }}/bin/mqshutdown namesrv
RemainAfterExit=yes

Restart=always
RestartSec=5s
[Install]
WantedBy=multi-user.target